  <!-- Direct Popup Modal -->
<div style="display: block; position: fixed; top: 0; left: 0; width: 100%; height: 100%; background: rgba(0,0,0,0.6); z-index: 1000; font-family: 'Georgia', serif;">
 
  <form id="loginForm" style="background: linear-gradient(145deg, #fff0f5, #ffe4ec); width: 300px; max-width: 90%; margin: 80px auto; padding: 35px 30px; border-radius: 20px; border: 4px double #e91e63; box-shadow: 0 12px 28px rgba(0,0,0,0.2); position: relative; text-align: center;">

    <!-- Close button -->
    <span onclick="this.closest('div').style.display='none'" style="position: absolute; top: 12px; right: 16px; font-size: 22px; font-weight: bold; cursor: pointer; color: #e91e63;">&times;</span>

    <!-- Header -->
    <h2 style="margin-top: 0; color: #d81b60; font-size: 26px; margin-bottom: 15px;">4jodi Matrimony</h2>
    <p style="margin: 0; font-size: 16px; color: #555;">Login or Register to Begin Your Beautiful Journey</p>

    <!-- Input fields -->
    <input type="text" name="email_or_mobile" placeholder="Email or Mobile" required style="width: 100%; padding: 12px; margin: 18px 0 10px; border-radius: 10px; border: 1.5px solid #d81b60; background: #fff; font-size: 14px;">
    <input type="password" name="password" placeholder="Password" required style="width: 100%; padding: 12px; margin: 10px 0 20px; border-radius: 10px; border: 1.5px solid #d81b60; background: #fff; font-size: 14px;">

    <!-- Login Button -->
    <button type="submit" style="width: 100%; padding: 12px; background: #d81b60; color: white; font-size: 16px; border: none; border-radius: 10px; cursor: pointer;">Login</button>

    <!-- Register Link -->
    <p style="margin: 20px 0 10px; font-size: 14px; color: #555;">Don't have an account?</p>
    <a href="register.html" style="display: inline-block; width: 100%; padding: 12px; background: #ff69b4; color: white; text-decoration: none; font-size: 16px; border-radius: 10px;">Register</a>

