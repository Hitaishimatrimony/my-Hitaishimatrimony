<script>
  function toggleAnswer(id, element) {
    // Get all answer elements
    var allAnswers = document.querySelectorAll('[id^="answer"]');
    // Get all plus/minus spans (they are the third child of the clickable div)
    var allSpans = document.querySelectorAll('[onclick^="toggleAnswer"] > span:nth-child(3)');
    
    // Close all answers and reset all spans to '+'
    allAnswers.forEach(function(answer) {
      answer.style.display = 'none';
    });
    allSpans.forEach(function(span) {
      span.textContent = '+';
    });
    
    // Toggle the clicked answer
    var target = document.getElementById(id);
    var targetSpan = element.children[2];
    
    if (target.style.display === 'none' || target.style.display === '') {
      target.style.display = 'block';
      targetSpan.textContent = '−';
    } else {
      target.style.display = 'none';
      targetSpan.textContent = '+';
    }
  }
 </script>


