   <script>
  function toggleAnswer(id, element) {
    var clickedAnswer = document.getElementById(id);
    var clickedIcon = element.children[2];
    var isOpen = clickedAnswer.style.display === 'block';

    // Close all answers and reset icons
    document.querySelectorAll('[id^="answer"]').forEach(function(answer) {
      answer.style.display = 'none';
    });
    document.querySelectorAll('[onclick^="toggleAnswer"] > span:nth-child(3)').forEach(function(span) {
      span.textContent = '+';
    });

    // If it was not open, open it now
    if (!isOpen) {
      clickedAnswer.style.display = 'block';
      clickedIcon.textContent = '−';
    }
  }
</script>

