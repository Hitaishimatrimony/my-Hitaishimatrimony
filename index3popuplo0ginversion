        <div style="font-size: 20px; color: #555;">Happy Couples</div>
      </div>

      <!-- Years of Experience Counter -->
      <div class="counter-item" style="padding: 30px; background: rgba(255,255,255,0.9); border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.1); transition: transform 0.3s ease, box-shadow 0.3s ease;">
        <div style="font-size: 48px; font-weight: 700; color: #e83e8c; margin-bottom: 10px;" class="count" data-count="11">0</div>
        <div style="font-size: 20px; color: #555;">Years of Experience</div>
